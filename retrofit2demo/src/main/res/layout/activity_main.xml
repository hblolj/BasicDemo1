<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:text="get"
        android:onClick="get"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="getWithPath"
        android:onClick="getWithPath"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="getWithParam"
        android:onClick="getWithParam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="getWithMap"
        android:onClick="getWithMap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="addUser"
        android:onClick="addUserWithPost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="editUserWithPostForm"
        android:onClick="editUserWithPostForm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:text="uploadPhotoWithPost"
        android:onClick="uploadPhotoWithPost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tv_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</LinearLayout>
