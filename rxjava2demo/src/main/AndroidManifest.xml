<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hblolj.rxjava2demo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SchdulerActivity">
        </activity>
        <activity android:name=".FlatMapActivity">
        </activity>
        <activity android:name=".DebounceAndSwitchMapActivity">
        </activity>
        <activity android:name=".ThrottleFirstActivity">
        </activity>
        <activity android:name=".MergeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".IntervalActivity">
        </activity>
        <activity android:name=".ImagerLoaderActivity">
        </activity>
    </application>

</manifest>